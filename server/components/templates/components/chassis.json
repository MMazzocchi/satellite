{% extends "components/instance_data.json" %}

{% block data %}
"name":        "{{ instance_data.name }} Chassis",
"color":       "{{ instance_data.color }}",
"width":        {{ instance_data.width }},
"length":       {{ instance_data.length }},
"height":       {{ instance_data.height }},
"cost":         {{ instance_data.cost }},
"description": "{{ instance_data.description }}"
{% endblock %}

{% block metrics %}
"Durability":   {{ instance_data.durability }},
"Weight":       {{ instance_data.weight }}
{% endblock %}
