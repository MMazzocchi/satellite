{
    {% if valid %}
    "valid": true,
    "name": "{{ name }}",
    "components": {
        "chassis":     {{ chassis }},
        "commDish":    {{ commDish }},
        "batteries":   {{ batteries }},
        "solarPanels": {{ solarPanels }},
        "storage":     {{ storage }},
        "sensors":     {{ sensors }},
        "processor":   {{ processor }},
        "fuelTank":    {{ fuelTank }},
        "thrusters":   {{ thrusters }}
    },
    "job": {{ job }}
    {% else %}
    "valid": false
    {% endif %}
}
